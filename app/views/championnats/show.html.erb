<div class="row">
    <div class="col.s12.m6">
      <div class="card">
        <div class="card-content">
          <span class='card-title'> <%= @championnat.name %></span>
          <div class="card-action">
            <%= link_to "<span class='fa fa-pencil'>Edit</span>".html_safe, edit_championnat_path( @championnat.id )%>
            <%= link_to "<span class='fa fa-close'>Delete</span>".html_safe,
                        championnat_path( @championnat.id ),
                        method: :delete,
                        data: { confirm: 'Confirmer la suppression ?' }%>
          </div>
        </div>
      </div>
    </div>
</div>
